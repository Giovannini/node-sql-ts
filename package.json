        "@types/lodash": "^4.14.123",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.0.0",
        "mocha": "*",
        "typescript": "^3.4.5",
        "ts-node": "^8.1.0",
        "tslint": "^5.16.0",
        "tslint-config-prettier": "^1.18.0"
    }
}
